{
    "name": "rodin/main",
    "semver": true,
    "source": {
        "type": "git",
        "url": "git@github.com:RodinJS/Rodin-Lib.git"
    },
    "commands": {
        "prebuild": "npm i",
        "build": "gulp clean; gulp js; gulp vendor;"
    },
    "output": {
        "./bundle": {
            "from": "./dist",
            "main": "core/index.js",
            "env": "prod"
        },
        "./source": {
            "from": "./src",
            "main": "core/index.js",
            "env": "dev"
        }
    }
}
