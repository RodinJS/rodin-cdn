{
    "name": "threejs/loaders",
    "semver": false,
    "source": {
        "type": "git",
        "url": "git@github.com:serghov/three-loaders.git"
    },
    "commands": {
        "build": "rollup -c rollup.config.js"
    },
    "output": {
        "./source": {
            "from": "./src",
            "main": "index.js",
            "env": "dev"
        },
        "./bundle": {
            "from": "./build",
            "main": "index.js",
            "env": "prod"
        }
    }
}
